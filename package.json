  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/plotly.js": "^3.0.9",
    "concurrently": "^9.1.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.1.0"
  }
